{
    "name": "ag-charts-enterprise-example",
    "version": "7.1.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "watch": "run-p watch:* start",
        "watch:ccm": "cd ../../charts-community-modules/ag-charts-community && npm run watch-esm",
        "watch:cem": "cd ../../charts-enterprise-modules/core && npm run watch-esm",
        "watch:enterprise-modules": "lerna run watch-esm --parallel --scope '@ag-charts-enterprise/**'",
        "start": "npx webpack serve --open",
        "install-hooks": "husky install .husky",
        "lint": "run-s lint:*",
        "lint:prettier": "npx prettier -c .",
        "lint:eslint": "npx eslint src/ --ext .js,.ts",
        "fix": "run-s fix:*",
        "fix:prettier": "npx prettier -w .",
        "fix:eslint": "npx eslint --fix src/ --ext .js,.ts",
        "bundle-enterprise": "node ./tools/bundle.js && webpack -c webpack.bundle.js"
    },
    "author": "sean@thelandsmans.com",
    "license": "ISC",
    "dependencies": {
        "@ag-charts-enterprise/core": "~7.1.0",
        "@ag-charts-enterprise/zoom": "~7.1.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.51.0",
        "@typescript-eslint/parser": "^5.51.0",
        "ag-charts-community": "~7.1.0",
        "eslint": "^8.35.0",
        "html-webpack-plugin": "^5.5.0",
        "husky": "^8.0.3",
        "lint-staged": "^13.1.2",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.8.3",
        "ts-loader": "^9.4.2",
        "typescript": "~4.9.3",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.11.1"
    },
    "lint-staged": {
        "*.{ts,js,md,json}": ".lint-staged/run.sh prettier",
        "*.{ts,js}": ".lint-staged/run.sh eslint"
    }
}
